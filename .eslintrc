{
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "max-len": ["error", { "code": 120 }], // 將行長度限制設置為 100 字符
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}
